option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "website.settings"
    "PYTHONPATH": "/opt/python/current/app/Django:$PYTHONPATH"
	"ALLOWED_HOSTS": ".elasticbeanstalk.com"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: Django/website/wsgi.py
    NumProcesses: 3
    NumThreads: 20
container_commands:
  01_migrate:
    command: "source /opt/python/run/venv/bin/activate && python Django/manage.py migrate --noinput"
    leader_only: true
  02_createsu:
    command: "source /opt/python/run/venv/bin/activate && python iotd/manage.py createsu"
    leader_only: true